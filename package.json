{
  "name": "@vasoyaprince14/sql-analyzer",
  "version": "1.5.2",
  "description": "ğŸš€ Enterprise SQL database analyzer with AI insights, security auditing, performance optimization, real-time monitoring, and beautiful reports",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "sql-analyzer": "dist/bin/enhanced-cli.js",
    "sqlanalyzer": "dist/bin/enhanced-cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "LICENSE",
    "examples",
    "docs"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc && npm run copy-assets",
    "build:prod": "tsc -p tsconfig.prod.json && npm run copy-assets && npm run optimize",
    "copy-assets": "cp -r examples dist/ && cp -r docs dist/ 2>/dev/null || true",
    "optimize": "npm run remove-sourcemaps && npm run compress-assets",
    "remove-sourcemaps": "find dist -name '*.map' -delete",
    "compress-assets": "find dist -name '*.png' -exec optipng -o2 {} \\; 2>/dev/null || true",
    "dev": "ts-node src/enhanced-sql-analyzer.ts",
    "cli": "ts-node bin/enhanced-cli.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts bin/**/*.ts",
    "lint:fix": "eslint src/**/*.ts bin/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"bin/**/*.ts\" \"*.md\"",
    "type-check": "tsc --noEmit",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint && npm run type-check",
    "prepack": "npm run build",
    "start": "node dist/bin/enhanced-cli.js",
    "docs:generate": "typedoc src --out docs/api",
    "docs:preview": "node scripts/render-preview.js",
    "release": "npm run build:prod && npm version patch && npm publish",
    "release:minor": "npm run build:prod && npm version minor && npm publish",
    "release:major": "npm run build:prod && npm version major && npm publish",
    "postinstall": "node -e \"console.log('\\nğŸ‰ Thanks for installing @vasoyaprince14/sql-analyzer v1.5.0!\\nğŸ“– Docs: https://github.com/vasoyaprince14/sql-optimizer\\nğŸ’ Support us: https://github.com/sponsors/vasoyaprince14\\nğŸ”” Check for updates: npm update @vasoyaprince14/sql-analyzer\\nğŸš€ New in v1.5.0: Enterprise features, ML predictions, compliance auditing, and predictive maintenance!\\n')\""
  },
