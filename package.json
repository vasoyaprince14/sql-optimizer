{
  "name": "sql-optimizer",
  "version": "0.1.0",
  "description": "A powerful CLI and Node.js tool for SQL query optimization and performance analysis",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "sqlopt": "dist/bin/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "cli": "ts-node bin/cli.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "start": "node dist/bin/cli.js"
  },
  "keywords": [
    "sql",
    "optimizer",
    "postgresql",
    "performance",
    "index",
    "query",
    "analysis",
    "cli",
    "database",
    "optimization"
  ],
  "author": "Prince Vasoya",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vasoyaprince14/sql-optimizer.git"
  },
  "bugs": {
    "url": "https://github.com/vasoyaprince14/sql-optimizer/issues"
  },
  "homepage": "https://github.com/vasoyaprince14/sql-optimizer#readme",
  "dependencies": {
    "commander": "^11.1.0",
    "chalk": "^5.3.0",
    "pg": "^8.11.3",
    "dotenv": "^16.3.1",
    "openai": "^4.20.1",
    "ora": "^7.0.1",
    "cli-table3": "^0.6.3",
    "figlet": "^1.7.0",
    "inquirer": "^9.2.12",
    "fs-extra": "^11.1.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/figlet": "^1.5.8",
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
} 