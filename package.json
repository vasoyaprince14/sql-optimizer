{
  "name": "@vasoyaprince14/sql-analyzer",
  "version": "1.0.0",
  "description": "ðŸš€ Enhanced SQL database analyzer with AI-powered insights, comprehensive security analysis, RLS policy auditing, and beautiful HTML reports",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "sql-analyzer": "dist/bin/enhanced-cli.js",
    "sqlanalyzer": "dist/bin/enhanced-cli.js"
  },
  "files": [
    "dist",
    "README.md", 
    "CHANGELOG.md",
    "LICENSE",
    "examples",
    "docs"
  ],
  "scripts": {
    "build": "tsc && npm run copy-assets",
    "copy-assets": "cp -r examples dist/ && cp -r docs dist/ 2>/dev/null || true",
    "dev": "ts-node src/enhanced-sql-analyzer.ts",
    "cli": "ts-node bin/enhanced-cli.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts bin/**/*.ts",
    "lint:fix": "eslint src/**/*.ts bin/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"bin/**/*.ts\" \"*.md\"",
    "type-check": "tsc --noEmit",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint && npm run type-check",
    "prepack": "npm run build",
    "start": "node dist/bin/enhanced-cli.js",
    "docs:generate": "typedoc src --out docs/api",
    "release": "npm version patch && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish"
  },
  "keywords": [
    "sql",
    "database",
    "analyzer",
    "postgresql",
    "performance",
    "security",
    "optimization",
    "rls",
    "row-level-security",
    "index",
    "query",
    "analysis",
    "cli",
    "audit",
    "health-check",
    "ai",
    "openai",
    "triggers",
    "procedures",
    "bloat",
    "cost-analysis",
    "report",
    "html",
    "dashboard",
    "npm-package",
    "typescript"
  ],
  "author": "Prince Vasoya",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vasoyaprince14/sql-optimizer.git"
  },
  "bugs": {
    "url": "https://github.com/vasoyaprince14/sql-optimizer/issues"
  },
  "homepage": "https://github.com/vasoyaprince14/sql-optimizer#readme",
  "dependencies": {
    "commander": "^11.1.0",
    "chalk": "^5.3.0",
    "pg": "^8.11.3",
    "dotenv": "^16.3.1",
    "openai": "^4.20.1",
    "ora": "^7.0.1",
    "cli-table3": "^0.6.3",
    "figlet": "^1.7.0",
    "inquirer": "^9.2.12",
    "fs-extra": "^11.1.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/figlet": "^1.5.8",
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.25.4",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
} 